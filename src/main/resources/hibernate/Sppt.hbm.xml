<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="lab.aikibo.model.Sppt" table="sppt_terhutang">
    <composite-id>
      <property-key name="nop" column="NOP" type="string" />
      <property-key name="thn" column="THN" type="string" />
    </composite-id>
    <property name="nama" type="string">
      <column name="NAMA" />
    </property>
    <property name="alamatOp" type="string">
      <column name="ALAMAT_OP" />
    </property>
    <property name="pokok" type="big_integer">
      <column name="POKOK" />
    </property>
    <property name="denda" type="big_integer">
      <column name="DENDA" />
    </property>
  </class>
</hibernate-mapping>
